{
    "manifest_version": 2,
    "name": "Kary's AdEscape",
    "author": "Pouya Kary",
    "incognito": "spanning",
    "version": "0.1",
    "description": "Let all those YouTube ads be skipped automatically",
    "icons": {
        "48": "icon/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["core.js"]
        }
    ]
  }